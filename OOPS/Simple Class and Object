#include <iostream>
using namespace std;

class Student {
public:
    string name;
    int age;

    void display() {
        cout << "Name: " << name << endl;
        cout << "Age: " << age << endl;
    }
};

int main() {
    Student s1;          // create object
    s1.name = "Habiba";
    s1.age = 18;

    s1.display();        // call function
    return 0;
}Simple Class and Object
