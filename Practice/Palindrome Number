#include <iostream>
using namespace std;

int main() {
    int num, temp, rev = 0, digit;
    cout << "Enter a number: ";
    cin >> num;

    temp = num;
    while (num != 0) {
        digit = num % 10;
        rev = rev * 10 + digit;
        num /= 10;
    }

    if (rev == temp)
        cout << "Palindrome number";
    else
        cout << "Not a palindrome";

    return 0;
}
