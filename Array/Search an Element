#include <iostream>
using namespace std;

int main() {
    int arr[5], key, found = 0;

    cout << "Enter 5 numbers: ";
    for (int i = 0; i < 5; i++)
        cin >> arr[i];

    cout << "Enter element to search: ";
    cin >> key;

    for (int i = 0; i < 5; i++) {
        if (arr[i] == key) {
            cout << "Element found at position " << i + 1;
            found = 1;
            break;
        }
    }

    if (!found)
        cout << "Element not found";

    return 0;
}
